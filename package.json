{
  "name": "coolcamper",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autotrack": "^2.4.1",
    "coolcamper-common": "^1.1.2",
    "core-js": "^3.1.3",
    "flag-icon-css": "^3.3.0",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "raf": "^3.4.1",
    "react": "^16.8.6",
    "react-dates": "^20.2.2",
