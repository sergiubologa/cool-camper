{
  "version": 2,
  "name": "cool-camper",
  "alias": ["coolcamper.ro", "www.coolcamper.ro"],
  "builds": [
    { "src": "api/index.js", "use": "@now/node-server" },
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "^/static/(.*)", "dest": "/static/$1" }
  ]
}
